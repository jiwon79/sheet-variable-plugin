{
  "name": "sheet-variable",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite -c ./vite.config.ui.ts",
    "build": "tsc && npm run build:ui && npm run build:plugin",
    "build:ui": "vite build -c ./vite.config.ui.ts --emptyOutDir",
    "build:plugin": "vite build -c ./vite.config.plugin.ts",
    "watch:ui": "vite build -c ./vite.config.ui.ts --watch",
    "watch:plugin": "tsc && vite build -c ./vite.config.plugin.ts --watch",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/node": "^20.4.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  },
  "dependencies": {
    "@figma/plugin-typings": "^1.68.0",
    "path": "^0.12.7",
    "postcss-url": "^10.1.3",
    "vite-plugin-generate-file": "^0.0.4",
    "vite-plugin-singlefile": "^0.13.5"
  }
}
